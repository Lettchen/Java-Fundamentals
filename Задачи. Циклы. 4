/*С помощью оператора while напишите программу вывода всех четных чисел в диапазоне
от 2 до 100 включительно.*/

package by.htp.hw.functional;

public class Evener {
    public static void main(String [] args){
        int i=2;
        while(i<=100){
            i++;
            if(i%2==0) {
                System.out.println(i);
            }
        }
    }
}
