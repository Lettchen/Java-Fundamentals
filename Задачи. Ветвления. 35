/*Вычислить число и месяц в невисокосном году по номеру дня.*/

package by.htp.hw.functional;
import java.util.Scanner;

public class DayNumber {
    public static void main(String [] args) {
        Scanner sc = new Scanner(System.in);
        int day = sc.nextInt();
        if(day>0 && day<=31){
            System.out.println(day + ".01");
        }
        else if(day>31 && day<=59){
            day=day-31;
            System.out.println(day + ".02");
        }
        else if(day>59 && day<=90){
            day=day-59;
            System.out.println(day + ".03");
        }
        else if(day>90 && day<=120){
            day=day-90;
            System.out.println(day + ".04");
        }
        else if(day>120 && day<=151){
            day=day-120;
            System.out.println(day + ".05");
        }
        else if(day>151 && day<=181){
            day=day-151;
            System.out.println(day + ".06");
        }
        else if(day>181 && day<=212){
            day=day-181;
            System.out.println(day + ".07");
        }
        else if(day>212 && day<=243){
            day=day-212;
            System.out.println(day + ".08");
        }
        else if(day>243 && day<=273){
            day=day-243;
            System.out.println(day + ".09");
        }
        else if(day>273 && day<=304){
            day=day-273;
            System.out.println(day + ".10");
        }
        else if(day>304 && day<=334){
            day=day-304;
            System.out.println(day + ".11");
        }
        else if(day>334 && day<=365){
            day=day-334;
            System.out.println(day + ".12");
        }
        else{
            System.out.println("Неправильно введён день");
        }
    }
}
